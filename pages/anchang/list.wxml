      <view class="filter-btn1">不限条件</view>
      <view class="filter-btn2">确认</view>
    </view>
  </view>

  <view class="xfList-zongjia filter dn">
    <view class="filter-hd">总价（元）</view>
    <view class="filter-bd">
      <view class="filter-ele">5000以下</view>
